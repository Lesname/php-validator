{
  "name": "lessname/validator",
  "description": "Validate your input",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://gitlab.com/api/v4/group/6632935/-/packages/composer/packages.json"
    }
  ],
  "require": {
    "php": "^8.4.0",

    "ext-json": "*",

    "lessname/documentor": "^0.3.0"
  },
  "require-dev": {
    "ext-intl": "*",

    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^2.0.0",
    "phpunit/phpunit": "^10.0.0",
    "vimeo/psalm": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "LessValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessValidatorTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
